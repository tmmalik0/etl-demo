{
    "name": "marquine/php-etl",
    "description": "Extract, Transform and Load data using PHP.",
    "keywords": ["etl", "extract", "transform", "load", "extraction", "transformation", "seed", "data", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Marquine",
            "email": "leonardomarquine@gmail.com"
        }
    ],
    "require": {
        "flow/jsonpath": "^0.3.1",
        "illuminate/container": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Marquine\\Etl\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marquine\\Etl\\EtlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
